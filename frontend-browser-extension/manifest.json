{
    "manifest_version": 3,
    "name": "API Monitor Extension",
    "version": "1.0",
    "description": "Monitors API requests.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage",
      "scripting"
    ],
    "host_permissions": [
         "<all_urls>"
    ],
   "background": {
    "service_worker": "src/background.ts"
},
"content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["src/contentScript.js"]
    }
],
"web_accessible_resources": [
    {
        "resources": ["src/contentScript.ts"],
        "matches": ["<all_urls>"]
    }
]
}
  