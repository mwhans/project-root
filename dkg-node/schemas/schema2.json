{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Example Schema 2",
    "type": "object",
    "properties": {
      "proposalId": {
        "type": "string",
        "description": "Identifier for a governance proposal."
      },
      "description": {
        "type": "string",
        "description": "Human-readable explanation of the proposal."
      },
      "status": {
        "type": "string",
        "enum": ["pending", "active", "closed"],
        "description": "Current status of the proposal."
      }
    },
    "required": ["proposalId", "description", "status"]
  }
  